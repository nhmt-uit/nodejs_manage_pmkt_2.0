{
  "name": "nodejs-source-structure",
  "version": "1.0.0",
  "description": "Build Nodejs Structure",
  "main": "app.js",
  "scripts": {
    "dev": "ts-node-dev -r esm --respawn --transpileOnly ./src/app.ts",
    "start": "nodemon -r esm src/app.js",
    "start:pm2": "pm2 start ecosystem.config.js",
    "restart:pm2": "pm2 start ecosystem.config.js",
    "stop:pm2": "pm2 stop all",
    "log:pm2": "pm2 log",
    "test": "mocha -r esm \"tests/api\"",
    "test:api": "mocha -r esm \"tests/api\"",
    "test:socket": "mocha -r esm \"tests/socket\"",
    "test:feature": "mocha -r esm \"tests/features\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/compression": "^1.0.1",
    "@types/connect-mongo": "^0.0.43",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/express-session": "^1.15.14",
    "@types/express-validator": "^3.0.0",
    "@types/jsonwebtoken": "^8.3.3",
    "@types/moment": "^2.13.0",
    "@types/moment-timezone": "^0.5.12",
    "@types/mongoose": "^5.5.17",
    "@types/socket.io": "^2.1.2",
    "@types/socket.io-client": "^1.4.32",
    "@types/winston": "^2.4.4",
    "esm": "^3.2.25",
    "http": "^0.0.0",
    "ts-node-dev": "^1.0.0-pre.42",
    "typescript": "^3.6.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "mocha": "^6.2.0"
  }
}
